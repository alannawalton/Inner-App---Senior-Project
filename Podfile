  pod 'FirebaseUI/Facebook'
  pod 'FirebaseUI/Twitter'
  pod 'Firebase/Database'
  pod 'RSKImageCropper'

  target 'InnerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'InnerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
